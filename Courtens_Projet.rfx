     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 150 compatibility


/* Pour appliquer le shader a plusieurs objets une pass est utilisé pour chaque objet 
afin de le déplacer et ainsi rendre plusieurs objets dans la scène. Cette méthode n'est pas procédurale 
mais permet d'avoir un rendu avec plusieurs objets affichés */


uniform sampler2D position; 
uniform sampler2D normal;
uniform sampler2D diffuse;

uniform vec4 ka;
uniform vec4  ks;
uniform float shininess;

uniform vec4 Id;
uniform vec4 Is;
uniform vec4 Ia;

uniform vec3 lightPos;

uniform vec4 viewDir;

in vec2 texCoord;

void main(void) {
  vec3 pos = texture(position, texCoord).xyz;
  vec3 norm = texture(normal, texCoord).xyz;
  vec3 kd = texture(diffuse, texCoord).xyz;
  
  // vec3 I = lightColor.rgb * lightPower;
  vec3 ldir = normalize(lightPos - pos);
  
  float diffuseIntensity = max(dot(norm,ldir), 0.0);
  
  vec4 diffuse = Id * vec4(kd,0.0) * diffuseIntensity;
  
  vec3 vd = normalize(viewDir.xyz);
  vec3 rd = normalize(reflect(ldir, norm));
  
  float specularIntensity = pow(max(dot(rd, vd), 0.0), shininess);
  
  vec4 specular = Is * ks * specularIntensity;
  
  vec4 ambiant = Ia * ka;
  
  gl_FragColor = diffuse + specular + ambiant;
  
}
]]></RmShaderCode></RmGLShader>
